
# passenger config file for the 'mukif' app
  # RackBaseURI requires symlink in public dir, check deploy recipe
  # check also http://www.modrails.com/documentation/Users%20guide.html#deploying_rack_to_sub_uri

<VirtualHost *:80>
  ServerName <%= passenger_server_name %>
  DocumentRoot <%= passenger_document_root %>
  RackEnv production
  PassengerPoolIdleTime 0
  RackBaseURI /tracker
</VirtualHost>
